@model MilestoneTracker.Contracts.TeamInfo
<div>
    <ul id="members">
        @foreach (var member in Model.TeamMembers)
        {
            <li><div class="row"><span class="glyphicon-bold">@member</span> <button type="button" class="btn btn-danger" onclick="removeMember(this.id)">Remove</button></div></li>
        }
    </ul>
    <div class="input-group-addon"></div>
</div>
<script type="text/javascript">
    var items = $('#members').getElementsByTagName('li');
    for (var i = 0; i < items.length; i++) {
        var memberButton = items[i].getElementsByTagName('button');
        memberButton.onclick = removeMember;
    }
</script>